{
  "manifest_version": 3,
  "name": "Make Twitter Great Again",
  "description": "Small dumb extension to replace DOGE by Twitter bird",
  "author": "anaumenko83@gmail.com", 
  "version": "1.0.0",
  "content_scripts": [
    {
      "js": ["mtga.js"],
      "matches": ["https://twitter.com/*"],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}